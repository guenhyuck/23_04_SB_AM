package com.KoreaIT.cgh.demo.util;

public class Ut {

	public static boolean empty(Object obj) {
		if(obj == null) {
			return true;
		}
		if(obj instanceof String == false) { // obj instanceof String 오브젝트가 스트링이야?
		   String str = (String) obj;
		   return str.trim().length()==0;
		}
		
		return false;
	}

}
